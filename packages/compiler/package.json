{
  "name": "@openfn/compiler",
  "version": "0.0.2",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./worker": {
      "import": {
        "types": "./dist/worker/index.d.ts",
        "default": "./dist/worker.js"
      }
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "pnpm mocha test/**/*.spec.ts",
    "build": "rimraf dist/ && node --loader=tsm esbuild.ts prod",
    "watch": "node esbuild.ts watch"
  },
  "keywords": [],
  "author": "Stuart Corbishley <stuart@openfn.org>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.45",
    "@types/node-localstorage": "1.3.0",
    "@typescript/vfs": "1.4.0",
    "chai": "4.3.6",
    "cross-fetch": "3.1.5",
    "esbuild": "0.14.54",
    "execa": "6.1.0",
    "mocha": "10.1.0",
    "node-localstorage": "2.2.1",
    "rimraf": "3.0.2",
    "threads": "1.7.0",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "tsm": "2.2.2",
    "typescript": "4.8.4",
    "url-join": "5.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "typesVersions": {
    "*": {
      "worker": ["dist/worker/index.d.ts"]
    }
  }
}
