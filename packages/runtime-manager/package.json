{
  "name": "@openfn/runtime-manager",
  "version": "0.0.15",
  "description": "An example runtime manager service.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "pnpm ava",
    "test:types": "pnpm tsc --noEmit --project tsconfig.json",
    "build": "tsup --config ../../tsup.config.js src/index.ts src/mock-worker.ts --no-splitting",
    "build:watch": "pnpm build --watch",
    "serve": "nodemon -e ts --exec 'tsm --experimental-vm-modules --no-warnings src/server/index.ts'"
  },
  "author": "Open Function Group <admin@openfn.org>",
  "license": "ISC",
  "dependencies": {
    "@openfn/compiler": "workspace:^0.0.16",
    "@openfn/language-common": "2.0.0-rc3",
    "@openfn/runtime": "workspace:^0.0.11",
    "@types/koa": "^2.13.5",
    "@types/workerpool": "^6.1.0",
    "koa": "^2.13.4",
    "workerpool": "^6.2.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.5",
    "@types/node": "^17.0.31",
    "@types/workerpool": "^6.1.0",
    "ava": "^4.2.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "tsm": "^2.2.2",
    "tsup": "^6.2.3",
    "typescript": "^4.6.4"
  }
}
